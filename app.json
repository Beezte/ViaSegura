{
  "expo": {
    "name": "ViaSegura",
    "slug": "ViaSegura",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon-viasegura.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#007BFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bazante.ViaSegura"
    },
    "android": {
      "adaptiveIcon": {
        "orientation": "portrait",
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bazante.ViaSegura"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-Black.otf",
            "./assets/fonts/Inter-Bold.otf",
            "./assets/fonts/Inter-ExtraBold.otf",
            "./assets/fonts/Inter-ExtraLight.otf",
            "./assets/fonts/Inter-Light.otf",
            "./assets/fonts/Inter-Medium.otf",
            "./assets/fonts/Inter-Regular.otf",
            "./assets/fonts/Inter-SemiBold.otf",
            "./assets/fonts/Inter-Thin.otf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "86850bd3-eb4e-4fc9-b113-8356ff15a276"
      }
    }
  }
}
